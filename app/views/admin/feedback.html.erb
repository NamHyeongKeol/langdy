<div class="container">
  <div class="row">
    <div class="col-md-3">
      <div class="list-group">
        <%= render partial: '/admin/menu_list' %>
      </div>
    </div>

    <div class="col-md-9">
      <div class="col-md-12">
        <h2> Feedbacks </h2>
        <table class="table table-striped table-hover ">
          <thead>
            <tr>
              <th class="col-md-3">Name</th>
              <th class="col-md-9">Feedback</th>
            </tr>
          </thead>
          <tbody>
            <% @feedbacks.each do |f| %>
              <tr>
                <td><%= f.user.name %></td>
                <td><%= f.text %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>

    </div>

  </div>
</div>

