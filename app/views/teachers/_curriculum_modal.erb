<div class="modal fade in" id="curriculumModal" tabindex="-1" role="dialog" aria-labelledby="curriculumLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="curriculumLabel"><%= t "teacher.curriculum.title" %></h4>
      </div>
      <div class="modal-body curriculumModal">
        <ul id="tree-tab" class="nav nav-tabs">
          <li class="active"><a data-toggle="tab" href="#tree-ko">한국어</a></li>
          <li><a data-toggle="tab" href="#tree-ch">中文</a></li>
          <!--<li><a data-toggle="tab" href="#tree-en">English</a></li>-->
        </ul>
        
        <div id="tree-wrapper" class="tab-content">
          <div id="tree-ko" class="tab-pane fade active in"></div>
          <div id="tree-ch" class="tab-pane fade"></div>
          <!--<div id="tree-en" class="tab-pane fade"></div>-->
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal"><%= t "teacher.curriculum.cancel" %></button>
        <button id="curri_btn" type="button" class="btn btn-primary" data-dismiss="modal"><%= t "teacher.curriculum.select" %></button>
      </div>
    </div>
  </div>
</div>
